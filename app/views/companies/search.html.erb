<h2>検索フォーム</h2>
<%= search_form_for @q,url:result_companies_path do |f| %>

  <div class="field">
    <%= f.label :id ,"企業コード" %></br>
    <%= f.search_field :id_eq %>
  </div>
  
  <div class="field">
    <%= f.label :name %></br>
    <%= f.search_field :name_cont %>
  </div>
  
  <div class="field">
    <%= f.label :phone_number %></br>
    <%= f.search_field :phone_number_cont %>
  </div>
  <div class="field">
    <%= f.label :address %></br>
    <%= f.search_field :address_cont %>
  </div>
  
  <div class="field">
    <%= f.label :industry %></br>
    <%= f.search_field :industry_cont %>
  </div>
  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
<br/>
<%= link_to '会社一覧へ戻る', companies_path %>
